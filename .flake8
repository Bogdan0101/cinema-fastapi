[flake8]
inline-quotes = "
ignore = E203, E231, E266, W503, ANN101, ANN102, ANN401, N807, N818, VNE003
max-line-length = 119
max-complexity = 18
select = B,C,E,F,W,T4,B9,ANN,Q0,N8,VNE
exclude =
    .venv,
     venv,
     .git,
     __pycache__

extend-exclude =
    src/tests/*,
    src/database/migrations/*
